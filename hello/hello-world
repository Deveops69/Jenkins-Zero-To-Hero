pipeline {
  agent {
    docker { image 'node:16-alpine' }
  }
  stages {
    stage('Test1') {
      steps {
        sh 'node --version'
      }
    }
stage('helloworld----') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
         sh 'hello-world'
      }
    }

 
  }

}

